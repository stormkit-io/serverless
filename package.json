{
  "name": "@stormkit/serverless",
  "version": "1.0.3",
  "description": "Export node.js applications into serverless compatible functions",
  "main": "serverless.js",
  "types": "serverless.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --onlyChanged",
    "build": "rm -rf dist/ && rollup --config rollup.config.ts && rm -rf dist/types",
    "np": "npm run build && np --no-publish && cp ./package.json ./dist/package.json && cd ./dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stormkit-io/serverless.git"
  },
  "exports": {
    ".": "./serverless.js",
    "./dev-server": "./dev-server.js",
    "./router": "./router.js",
    "./presets": "./presets.js",
    "./datastore": "./datastore.js"
  },
  "keywords": [
    "serverless",
    "stormkit",
    "aws",
    "lambda"
  ],
  "author": "@stormkitio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stormkit-io/serverless/issues"
  },
  "homepage": "https://github.com/stormkit-io/serverless#readme",
  "dependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@nuxt/types": "^2.15.8",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.40",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "http-parser-js": "^0.5.6",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^28.1.0",
    "jiti": "^1.14.0",
    "next": "^12.2.1",
    "node-match-path": "^0.6.3",
    "np": "^7.6.1",
    "rollup": "^2.78.1",
    "rollup-plugin-dts": "^4.2.2",
    "ts-loader": "^9.3.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "yargs": "^17.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
