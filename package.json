{
  "name": "@stormkit/serverless",
  "version": "2.0.9",
  "description": "Export node.js applications into serverless compatible functions",
  "main": "serverless.js",
  "types": "serverless.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --onlyChanged",
    "build": "rm -rf dist/ && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript && rm -rf dist/types",
    "np": "npm run build && np --no-publish && cp ./package.json ./dist/package.json && cd ./dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stormkit-io/serverless.git"
  },
  "exports": {
    ".": "./serverless.js",
    "./api": "./api.js",
    "./aws": "./aws.js",
    "./middlewares": "./middlewares.js",
    "./middlewares/express": "./middlewares/express.js"
  },
  "keywords": [
    "serverless",
    "stormkit",
    "aws",
    "lambda"
  ],
  "author": "@stormkitio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stormkit-io/serverless/issues"
  },
  "homepage": "https://github.com/stormkit-io/serverless#readme",
  "dependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@google-cloud/functions-framework": "^3.3.0",
    "@nuxt/types": "^2.17.2",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/jest": "^28.1.8",
    "@types/node": "^17.0.45",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "http-parser-js": "^0.5.8",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^28.1.3",
    "jiti": "^1.21.0",
    "next": "^12.3.4",
    "node-match-path": "^0.6.3",
    "np": "^7.7.0",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^5.3.1",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "yargs": "^17.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
